[pma]

listen = /var/run/php-fpm/pma.sock

listen.owner = nginx
listen.group = nginx
listen.mode = 0666

user = nginx
group = nginx

pm = static

pm.max_children = 3
pm.start_servers = 1
pm.process_idle_timeout = 30s
;pm.min_spare_servers = 1
pm.max_spare_servers = 11
pm.max_requests = 0

slowlog = /home/www/log/php-fpm-slow.log

php_flag[display_errors] = on
php_admin_value[error_log] = /home/www/log/php-fpm-error.log
php_admin_flag[log_errors] = on
;php_admin_value[memory_limit] = 128M

; Set session path to a directory owned by process user
php_value[session.save_handler] = files
php_value[session.save_path]    = /var/lib/php/session-pma
